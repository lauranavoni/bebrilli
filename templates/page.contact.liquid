<div class= "page-custom_help">
  <div class="hero-unit_help">
    <div class="container-inner search-box search-box--hero-unit" data-search-box="">
      <div class="intro-title">

        {{ page.title }}

      </div>
      <div class="outersearchform">
        <form action="/search" method="get" class="search-header_help search" role="search">
          <input class="search-header__input search__input helpsearch"
            type="search" id = "query"
            name="q"
            placeholder="Type your question here..."
            aria-label="{{ 'general.search.placeholder' | t }}">
          <button class="search-header__submit search__submit btn--link site-header__icon" type="submit">
            <span class="icon__fallback-text">{{ 'general.search.submit' | t }}</span>
          </button>
    	</form>
      </div>
      <div class="header-contact">
      <div class="subtext_help rte">
        {{ page.content }}
      </div>    
      </div>
    </div>
  </div>
</div>
