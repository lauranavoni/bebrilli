<div  id = "FAQ" class="parent_container"> <div class="faq_accordian container">
   <h1 class= "heading">{{section.settings.title}}</h1>
  
   <div class= "faq_accordion_content_flex">
      {% for block in section.blocks %}
 
       <div id='{{ block.settings.title }}' class="tabcontent faq_acc_section">
         <div class= "mainheading">{{ block.settings.title }}</div>

        {% if block.settings.question1 != blank %} 
        <div class= "Individual">
          <div class="accordion">{{ block.settings.question1 | remove: '<p>'  | remove: '</p>'}}</div>
          <div class="panel_custom">{{ block.settings.answer1 | replace: "TM", "<sup>TM</sup>"}}</div>
        </div>
        {% endif %}
        {% if block.settings.question2 != blank %} 
        <div class= "Individual">
